{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		// pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/index"
		},
		{
			"path": "pages/mine/index",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/tool/index"
		},
		{
			"path": "pages/login/index",
			"style": {
				"navigationBarTitleText": "login"
			}
		}
	],
	"tabBar": {
		"borderStyle": "black",
		"list": [
			{
				"pagePath": "pages/home/index",
				"text": "首页",
				"color": "#374151",
				"selectedColor": "#F13858",
				"iconPath": "static/home.png",
				"selectedIconPath": "static/active_home.png"
			},
			{
				"pagePath": "pages/mine/index",
				"text": "我的",
				"color": "#374151",
				"selectedColor": "#F13858",
				"iconPath": "static/mine.png",
				"selectedIconPath": "static/active_mine.png"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "AML 智库",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"subPackages": [
		{
			"root": "pages/user-center",
			"pages": [
				{
					"path": "account-set/index",
					"style": {
						"navigationBarTitleText": "账户设置"
					}
				},
				{
					"path": "history-record/index",
					"style": {
						"navigationBarTitleText": "近期浏览记录"
					}
				},
				{
					"path": "collect/index",
					"style": {
						"navigationBarTitleText": "我的收藏"
					}
				},
				{
					"path": "edit-pwd/index",
					"style": {
						"navigationBarTitleText": "修改密码"
					}
				}
			]
		}
	]
}
